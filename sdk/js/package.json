{
  "name": "@stratiumdata/sdk",
  "version": "0.6.7",
  "type": "module",
  "description": "JavaScript SDK for Stratium platform with browser and Node.js support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./nodejs": {
      "types": "./dist/nodejs/index.d.ts",
      "import": "./src/nodejs/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./src/browser/index.js"
    }
  },
  "files": [
    "dist",
    "src/generated",
    "proto",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run proto:gen && rollup -c && npm run patch:types",
    "proto:gen": "protoc --plugin=protoc-gen-es=node_modules/.bin/protoc-gen-es --plugin=protoc-gen-connect-es=node_modules/.bin/protoc-gen-connect-es --es_out=src/generated --es_opt=target=ts --connect-es_out=src/generated --connect-es_opt=target=ts --proto_path=proto proto/services/key-access/key-access.proto proto/services/key-manager/key-manager.proto proto/services/platform/platform.proto proto/models/ztdf.proto proto/models/stanag4774.proto",
    "patch:types": "node scripts/patch-types.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "docs": "jsdoc -c jsdoc.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "stratium",
    "zero-trust",
    "encryption",
    "access-control",
    "ztdf",
    "abac",
    "policy"
  ],
  "author": "Stratium",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratiumdata/javascript-sdk"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "^1.7.0",
    "@connectrpc/connect-web": "^1.7.0",
    "axios": "^1.6.0",
    "jose": "^5.2.0",
    "jszip": "^3.10.1",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1"
  },
  "optionalDependencies": {
    "@connectrpc/connect-node": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@connectrpc/protoc-gen-connect-es": "^1.7.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.0",
    "rollup": "^4.6.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
